print ("this is a calc")
print ("enter first num")
num1=int(input())
print ("enter your desired operator")

if num1==5:
    print("we have found u cheating")
else:
    pop=input()

    print ("enter your second number")
    num2=int(input())

    if pop == "+":
        print ("your answer is",num1+num2)

    if pop== "-":
        print("your answer is",num1-num2)

    if pop=="/":
        print("your answer is", num1/num2)

    if pop=="*":
        print("your answer is", num1*num2)
